@model FamilyPhotos.ViewModel.PhotoViewModel

<div class="page-header">
    <h2>Kép Modositása </h2>
</div>
    @using (Html.BeginForm("Edit", "Photo", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal" }))
    {
        @Html.HiddenFor(m => m.Id)
        @Html.EditorForModel();
        <div class="col-xs-offset-2">
            @Html.ActionLink("Vissza a képek listájára", "Index", null, htmlAttributes: new { @class = "btn btn-default" })
            <input type="submit" value="Modositás" class="btn-primary" />
        </div>


    }
